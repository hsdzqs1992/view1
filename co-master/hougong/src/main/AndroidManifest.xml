<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhuye.hougong">

    <!-- Required -->
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:name=".HouGongApplition"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".view.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.RegeistActivity" />
        <activity android:name=".view.ForgetPassActivity" />
        <activity android:name=".view.ForgetPass2Activity" />
        <activity android:name=".view.PersonDetailActivity" />
        <activity android:name=".view.SettingsActivity" />
        <activity android:name=".view.FeedBackActivity" />
        <activity android:name=".view.GuanZhuActivity" />
        <activity android:name=".view.FansActivity" />
        //qq
        <activity
            android:name="com.tencent.tauth.AuthActivity"
            android:launchMode="singleTask"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data android:scheme="tencent你的AppId" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.tencent.connect.common.AssistActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:theme="@android:style/Theme.Translucent.NoTitleBar" />
        <activity android:name=".view.MyWaletActivity" />
        <activity android:name=".view.ChongZhiActivity" />
        <activity android:name=".view.TiXianActivity" />
        <activity android:name=".view.JiaoYiJiLvActivity" />
        <activity android:name=".view.ShouZhiJiLvActivity" />
        <activity android:name=".view.ZiPaiActivity" />
        <activity android:name=".view.YanQingJiangLiActivity" />
        <activity android:name=".view.BlackNumberActivity" />
        <activity android:name=".view.LookMeActivity" />
        <activity android:name=".view.WhoSendLiWuActivity" />
        <activity android:name=".view.ShengVIP1Activity" />
        <activity android:name=".view.ShengJiVIP2Activity" />
        <activity android:name=".view.MyFriendsActivity" />
        <activity android:name=".view.PersonHomePageActivity" />
        <activity android:name=".view.OtherPersonDetailActivity" />
        <activity android:name=".view.PingLunActivity" />
        <activity android:name=".view.FaBuActivity" />
        <activity android:name=".view.SelectPictureActivity" />
        <activity android:name=".view.SelectCityActivity" />
        <activity android:name=".view.SearchActivity" />
        <activity android:name=".view.ChatActivity" />
        <activity android:name=".view.ZhuanQianGongActivity" />

        <meta-data
            android:name="EASEMOB_APPKEY"
            android:value="1162171124115414#hougong" />
        <!-- 声明SDK所需的service SDK核心功能 -->
        <service
            android:name="com.hyphenate.chat.EMChatService"
            android:exported="true" />
        <service
            android:name="com.hyphenate.chat.EMJobService"
            android:exported="true"
            android:permission="android.permission.BIND_JOB_SERVICE" />
        <!-- 声明SDK所需的receiver -->
        <receiver android:name="com.hyphenate.chat.EMMonitorReceiver">
            <intent-filter>
                <action android:name="android.intent.action.PACKAGE_REMOVED" />

                <data android:scheme="package" />
            </intent-filter>
            <!-- 可选filter -->
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="android.intent.action.USER_PRESENT" />
            </intent-filter>
        </receiver>

        <activity android:name=".view.BangDingYinHangActivity" />
        <activity android:name=".view.ShenQingActivity"></activity>
    </application>

</manifest>